{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest":true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "class-methods-use-this":"off",
        "no-underscore-dangle":"off",
        "no-unused-vars": "off",
        "no-param-reassign":"off"
    }
}
